#!/bin/bash
function commit {
  git status
  git pull
  echo "$1 message (empty to skip):"
  read msg
  git add . > /tmp/gitaddlog 2> /tmp/gitadderror
  git commit -m "$msg" > /tmp/gitcommitlog 2> /tmp/gitcommiterror
  git push > /tmp/gitpushlog 2> /tmp/gitpusherror
}

cd $HOME/.dotfiles
commit dotfiles
cd $HOME/vimwiki
commit vimwiki
